firstentry := proc(nrchars, nrBrchars, DM, test)
local j:=0;
local i:=1;
local entryfound:=Array(1..nrchars, fill = false, datatype=boolean);


while not (i=nrchars) do
      j:=0;
  while not entryfound[i] do
        test[i]:=DM[i,(nrBrchars-j)];
#         test[i]:=DM[i,j+1];
        if test[i]<>0 then 
        entryfound[i]:=true;
        end if;
	j:=j+1; 
  end do;
entryfound[i]:=true;
i:=i+1
    end do;





end proc;