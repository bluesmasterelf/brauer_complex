#this is a collection of 66 character families which depend on various parameter sets t1-s2
#they should each accept order, parameters and coefficients. Based on order and parameter they write a coefficient into the appropriate index in an array. 

chi1 := proc(coefficient,column) #column is an array

     column[1]=column[1] + coefficient;

end proc;

chi2 := proc(coefficient,column)	

     column[2]= column[2]+coefficient;

end proc;

chi3 := proc(coefficient,column)

     column[3]=column[3]+coefficient;

end proc;

4 := proc(coefficient,column)

     column[4]=column[4]+coefficient;

end proc;

chi5 := proc(coefficient,column)

     column[5]=column[5]+coefficient;

end proc;

chi6 := proc(coefficient,column)	

     column[6]=column[6]+coefficient;

end proc;

chi7 := proc(coefficient,column)	

     column[7]=column[7]+coefficient;

end proc;

chi8 := proc(coefficient,column)	

     column[8]=column[8]+coefficient;

end proc;

chi9 := proc(coefficient,column)	

     column[9]=column[9]+coefficient;

end proc;

chi10 := proc(coefficient,column)	

     column[10]=column[10]+coefficient;

end proc;

chi11 := proc(coefficient,column)	

     column[11]=column[11]+coefficient;

end proc;

chi12 := proc(coefficient,column)	

     column[12]=column[12]+coefficient;

end proc;

chi13 := proc(coefficient,parameter,order,column)

     column[12+parameter]=column[12+parameter]+coefficient;

end proc;

