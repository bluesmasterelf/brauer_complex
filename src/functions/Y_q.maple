#      Y_q[idx,2]:=2*(order-1)-Y_q[order^3-idx+1,2];
#      Y_q[idx,3]:=  (order-1)-Y_q[order^3-idx+1,3];
#      idx:=idx+1;
#end do;

return Y_q;

end proc;